{
  "name": "modsen-art-museum",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "axios": "^1.7.2",
    "formik": "^2.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-router-dom": "^6.25.1",
    "styled-components": "^6.1.12",
    "yup": "^1.4.0"
  },
  "scripts": {
